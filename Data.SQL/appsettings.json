{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },
  "SqlExpressConfig": {
    "DataSource": "${SQL_DATASOURCE}",
    "InitialCatalog": "${SQL_INITIAL_CATALOG}",
    "UserId": "sa",
    "IntegratedSecurity": "${SQL_INTEGRATED_SECURITY}",
    "TrustServerCertificate": "${SQL_TRUST_CERTIFICATE}",
    "Password": "${SQL_PASSWORD}"
  }
}
